#---------------------------
#   my-root
#---------------------------
# - used to sign our intermediate cert
#
name = 'my-root'
group = 'ca'
service = 'my-root' 
signing_ca = 'self'
dane_tls = []

[KeyOpts] 
    ktype = 'ec'
    ec_algo = 'secp384r1'

[X509]
    CN = "my-root"
    O = "Example Certs"
    C = "US"
    L = ''
    ST = ''
    email = 'hostmaster@example.com'
    sans = []

[CA]
    sign_end_days = 3650
    digest = 'sha384'

